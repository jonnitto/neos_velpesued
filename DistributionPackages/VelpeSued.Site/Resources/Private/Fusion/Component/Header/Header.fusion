prototype(VelpeSued.Site:Component.Header) < prototype(Neos.Fusion:Component) {
    homeLink = Neos.Neos:NodeUri {
        node = ${site}
    }

    renderer = afx`
        <header class="main-header">
            <a class="logo" href={props.homeLink}>BRAND NAME</a>
            <VelpeSued.Site:Component.Navigation startingPoint={site} />
        </header>
    `
}
